@css {
    body {
        background-color: #29282C;
        font-family: dosis,Segoe UI,Lucida Grande,Helvetica,Microsoft YaHei,FreeSans,Arimo,Droid Sans,wenquanyi micro hei,Hiragino Sans GB,Hiragino Sans GB W3,Arial,sans-serif;
    }
}

a
    color rgb(41,222,210)
.intro-wrap
    display block
    h1
        color white
        font-size 60px
        font-weight 200
        text-align center
    .heading
        margin-bottom 10px
        margin-top 0
        display none
    .content
        p
            color #69676A
            font-size 30px
            text-align center
            span
                padding 0 3px
            a
                &:hover
                &:focus
                    text-decoration: none
    .scroll-label
        text-align center
        display none
        margin 0 auto
        position absolute
        bottom 40px
        color white
        left 47%
        font-size 80px
        cursor pointer
        @media screen and (min-width: 981px)
            left 49%
            font-size 40px
.scroll-back
    text-align center
    margin 50px auto
    font-size 80px
    cursor pointer
    color white
    @media screen and (min-width: 981px)
        font-size 40px
#first-page
    height 5000px
    display table
    margin 0 auto
#second-page
    display table
    margin 0 auto
    margin-top 100px
#third-page
    display table
    margin 0 auto
    margin-top 100px
#fourth-page
    display table
    margin 0 auto
    margin-top 100px
.experience-wrap
    width 980px
    margin 0 auto
    position relative
#experienceTimeLine
    width 800px
    height 900px
    margin 0 auto
    position relative
.legend
    display table
    margin 50px auto

.skillsList
    padding 0
    list-style none
    li
        display inline
        float right
    button
        border none
        width 130px
        height 130px
        margin-right 3px
        border-radius 10px
        color #fff
        font-size 18px
        &:hover
            opacity: 0.7

#skillChart
    float left
    width 830px
    height 500px

.projectTable
    th
        padding 10px
        color #fff
    font-size 40px
    margin 20px
    color #69676A
    strong
        padding 0 5px
    .projectTd
        font-size 23px
        padding-left 50px
        // letter-spacing 1px
    .subProjectTd
        font-size 18px
        padding-left 100px
.connect
    color #fff
    margin-top 50px
